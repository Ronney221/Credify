{
  "expo": {
    "name": "Credify",
    "slug": "Credify",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "credify",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.yourbundleid.credify",
      "infoPlist": {
        "LSApplicationQueriesSchemes": [
          "dunkindonuts",
          "uber"
        ]
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.yourpackage.credify",
      "intentFilters": [],
      "queries": [
        {
          "package": "com.dunkinbrands.otgo"
        },
        {
          "intent": [
            {
              "action": "android.intent.action.VIEW",
              "data": {
                "scheme": "dunkindonuts"
              }
            }
          ]
        },
        {
          "intent": [
            {
              "action": "android.intent.action.VIEW",
              "data": {
                "scheme": "uber"
              }
            }
          ]
        }
      ]
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}
